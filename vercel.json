{
  "version": 2,
  "env": {
    "SUPABASE_URL": "@SUPABASE_URL",
    "SUPABASE_SERVICE_KEY": "@SUPABASE_SERVICE_KEY"
  },
  "builds": [{ "src": "api/**/*.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/$1.js" },
    { "src": "/api/(.*)", "dest": "api/$1/index.js" }
  ]
}
